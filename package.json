{
  "name": "styled-jsx",
  "version": "2.1.3",
  "license": "MIT",
  "repository": "zeit/styled-jsx",
  "description": "Full CSS support for JSX without compromises",
  "files": [
    "dist",
    "lib",
    "server.js",
    "babel.js",
    "style.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "dev": "gulp",
    "prepublish": "gulp transpile",
    "test": "npm run lint && ava",
    "lint": "xo",
    "format": "prettier --single-quote --no-semi --write *.js {src,test}/*.js",
    "precommit": "xo --quiet && lint-staged"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "extends": "prettier",
    "ignores": [
      "lib/style-transform.js"
    ],
    "rules": {
      "capitalized-comments": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-types": "6.26.0",
    "convert-source-map": "1.5.0",
    "source-map": "0.6.1",
    "string-hash": "1.1.3",
    "stylis": "3.4.2",
    "stylis-rule-sheet": "0.0.6"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "eslint-config-prettier": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-benchmark": "^1.1.1",
    "human-size": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "mz": "^2.7.0",
    "prettier": "^1.5.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-jsdom": "^2.0.5",
    "xo": "^0.18.2"
  },
  "peerDependencies": {
    "react": "15.x.x || 16.x.x"
  }
}
