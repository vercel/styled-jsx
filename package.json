{
  "name": "styled-jsx",
  "version": "0.5.7",
  "license": "MIT",
  "repository": "zeit/styled-jsx",
  "description": "Full CSS support for JSX without compromises",
  "files": [
    "dist",
    "lib",
    "server.js",
    "babel.js",
    "style.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "react"
    ]
  },
  "scripts": {
    "dev": "gulp",
    "prepublish": "gulp transpile",
    "test": "npm run lint && ava",
    "lint": "eslint *.js src/**/*.js",
    "prettier": "prettier --write --single-quote \"*.js\" \"src/**/*.js\""
    "prettier-standard": "prettier-standard-formatter --write --single-quote \"*.js\" \"src/**/*.js\"",
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier"
    ]
  },
  "dependencies": {
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-traverse": "6.21.0",
    "babylon": "6.14.1",
    "convert-source-map": "1.3.0",
    "escape-string-regexp": "1.0.5",
    "object.entries": "1.0.4",
    "source-map": "0.5.6",
    "string-hash": "1.1.1",
    "stylis": "1.1.11"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.16.0",
    "babel-register": "6.18.0",
    "benchmark": "2.1.3",
    "eslint": "^3.17.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-benchmark": "1.1.1",
    "human-size": "1.1.0",
    "mz": "2.6.0",
    "prettier": "^0.21.0",
    "prettier-standard-formatter": "^0.222222222222222.333333333333333",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "peerDependencies": {
    "react": "15.x.x"
  }
}
