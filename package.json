{
  "name": "styled-jsx",
  "version": "2.2.1",
  "license": "MIT",
  "repository": "zeit/styled-jsx",
  "description": "Full CSS support for JSX without compromises",
  "files": [
    "dist",
    "lib",
    "server.js",
    "babel.js",
    "style.js",
    "css.js"
  ],
  "babel": {
    "presets": [
      "@babel/preset-es2015",
      "@babel/preset-stage-3",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "dev": "gulp",
    "prepublish": "gulp transpile",
    "test": "npm run lint && ava",
    "lint": "xo",
    "format": "prettier --single-quote --no-semi --write *.js {src,test}/*.js",
    "precommit": "xo --quiet && lint-staged"
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": "inherit"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "extends": "prettier",
    "ignores": [
      "lib/style-transform.js"
    ],
    "rules": {
      "capitalized-comments": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.0.0-beta.32",
    "@babel/types": "7.0.0-beta.32",
    "convert-source-map": "1.5.1",
    "source-map": "0.6.1",
    "string-hash": "1.1.3",
    "stylis": "3.4.5",
    "stylis-rule-sheet": "0.0.7"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.32",
    "@babel/core": "7.0.0-beta.32",
    "@babel/plugin-transform-runtime": "7.0.0-beta.32",
    "@babel/preset-es2015": "7.0.0-beta.32",
    "@babel/preset-react": "7.0.0-beta.32",
    "@babel/preset-stage-3": "7.0.0-beta.32",
    "@babel/register": "7.0.0-beta.32",
    "@babel/runtime": "7.0.0-beta.32",
    "ava": "^0.19.1",
    "babel-preset-minify": "0.2.0",
    "benchmark": "^2.1.4",
    "eslint-config-prettier": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "8.0.0-beta.0",
    "gulp-benchmark": "^1.1.1",
    "human-size": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "mz": "^2.7.0",
    "prettier": "^1.5.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-jsdom": "^2.0.5",
    "xo": "^0.18.2"
  },
  "peerDependencies": {
    "react": "15.x.x || 16.x.x"
  }
}
