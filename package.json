{
  "name": "styled-jsx",
  "version": "0.0.7",
  "license": "MIT",
  "repository": "zeit/styled-jsx",
  "description": "Full CSS support for JSX without compromises",
  "main": "./dist/flush.js",
  "files": [
    "dist",
    "lib",
    "babel.js",
    "flush.js",
    "inject.js",
    "memory.js"
  ],
  "dependencies": {
    "babel-plugin-syntax-jsx": "^6.18.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.16.0",
    "babel-register": "^6.18.0",
    "fs-promise": "^1.0.0",
    "xo": "^0.17.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "react"
    ]
  },
  "scripts": {
    "prebuild": "mkdir -p dist",
    "build": "babel src -d dist",
    "build:watch": "npm run build -- --watch",
    "dev": "npm run build:watch",
    "lint": "xo",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "npm run lint && npm run test:jsx && npm run test:styles",
    "test:jsx": "ava test/index.js",
    "test:styles": "ava test/styles.js"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "ignores": [
      "lib/**",
      "test/fixtures/**"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ],
      "no-eq-null": 0,
      "import/no-unresolved": 0,
      "new-cap": 0
    }
  }
}
